#include <iostream>
using namespace std;

#define MAX 20

int S, C;
int grafo[MAX][MAX];
bool visitado[MAX];

bool cicloHamiltoniano(int atual, int inicio, int count) {
    // Se já visitou todas as salas e há caminho de volta → ciclo encontrado
    if (count == S && grafo[atual][inicio] == 1) 
        return true;

    // Testa todas as salas como próximo passo
    for (int prox = 0; prox < S; prox++) {
        if (grafo[atual][prox] == 1 && !visitado[prox]) {
            visitado[prox] = true;

            // Chama recursão
            if (cicloHamiltoniano(prox, inicio, count + 1))
                return true;

            // Backtrack (desmarca)
            visitado[prox] = false;
        }
    }
    return false;
}

bool existeCicloHamiltoniano() {
    // Tenta iniciar o ciclo em cada vértice
    for (int inicio = 0; inicio < S; inicio++) {
        for (int i = 0; i < S; i++)
            visitado[i] = false;

        visitado[inicio] = true;

        if (cicloHamiltoniano(inicio, inicio, 1))
            return true;
    }
    return false;
}

int main() {
    cin >> S >> C;

    // Zera matriz
    for (int i = 0; i < S; i++)
        for (int j = 0; j < S; j++)
            grafo[i][j] = 0;

    // Lê arestas (grafo não direcionado)
    for (int i = 0; i < C; i++) {
        int a, b, k;
        cin >> a >> b >> k;
        grafo[a][b] = 1;
        grafo[b][a] = 1;
    }

    // Verifica ciclo
    if (existeCicloHamiltoniano())
        cout << "Yes" << endl;
    else
        cout << "No" << endl;

    return 0;
}
