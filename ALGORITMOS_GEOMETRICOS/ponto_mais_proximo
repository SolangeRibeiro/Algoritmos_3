#include <iostream> 
using namespace std;

// Função geométrica que calcula a distância euclidiana ao quadrado
// entre dois pontos (x1, y1) e (x2, y2)
long long dist2(int x1, int y1, int x2, int y2) {
    long long dx = x1 - x2;   // diferença em x
    long long dy = y1 - y2;   // diferença em y
    return dx*dx + dy*dy;     // retorna o quadrado da distância
}

int main() {
    int N;
    cin >> N;   // Lê a quantidade de pontos de interesse

    int ux, uy; // Coordenadas do usuário (serão lidas depois)
    int x, y;   // Coordenadas de cada ponto lido

    // Variáveis que armazenam o ponto mais próximo encontrado
    long long bestDist = (long long)1e18; // distância inicial enorme
    int bestX = 0, bestY = 0;             // melhor ponto até o momento

    // Vetores para armazenar todos os pontos de interesse
    int px[110], py[110];

    // Loop para ler os N pontos
    for (int i = 0; i < N; i++) {
        cin >> px[i] >> py[i];  // Lê cada par (x, y)
    }

    // Lê a posição do usuário
    cin >> ux >> uy;

    // Loop para verificar qual ponto é o mais próximo
    for (int i = 0; i < N; i++) {
        // Calcula a distância quadrática entre o ponto e o usuário
        long long d = dist2(px[i], py[i], ux, uy);

        // Caso este ponto seja mais próximo do que o melhor até agora
        if (d < bestDist) {
            bestDist = d;       // Atualiza a melhor distância
            bestX = px[i];      // Atualiza o melhor x
            bestY = py[i];      // Atualiza o melhor y
        }
        // Caso a distância seja igual → aplicar desempates
        else if (d == bestDist) {
            // Primeiro desempate: menor valor de x
            if (px[i] < bestX ||
                // Segundo desempate: se x é igual, escolher menor y
                (px[i] == bestX && py[i] < bestY)) 
            {
                bestX = px[i];  // Atualiza x
                bestY = py[i];  // Atualiza y
            }
        }
    }

    // Mostra o ponto mais próximo respeitando os critérios de desempate
    cout << bestX << " " << bestY;

    return 0; // Finaliza o programa
}
